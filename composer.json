{
    "name": "laravel/laravel",
    "description": "The skeleton application for the Laravel framework.",
    "license": "MIT",
    "type": "project",
    "keywords": ["laravel", "framework"],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2.91",
        "filament/spatie-laravel-media-library-plugin": "^3.2.91",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.10.0",
        "laravel/tinker": "^2.9",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.3.2",
        "spatie/laravel-medialibrary": "^11.6.0",
        "spatie/laravel-sitemap": "^7.2.1",
        "spatie/laravel-sluggable": "^3.6",
        "tightenco/ziggy": "^2.2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.16.0",
        "laravel/sail": "^1.29.2",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1.1",
        "pestphp/pest": "^2.34.8",
        "pestphp/pest-plugin-laravel": "^2.4",
        "spatie/laravel-ignition": "^2.8.0",
        "spatie/laravel-ray": "^1.36.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan ziggy:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N -M"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": "./vendor/bin/pint"
    }
}
